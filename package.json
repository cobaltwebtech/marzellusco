{
  "name": "marzellus",
  "version": "2.0.0",
  "description": "Marzellus Astro Website",
  "author": "Cobalt Web Technologies",
  "license": "MIT",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "dev": "astro dev",
    "dev:cf": "pnpm build && pnpm wrangler dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "biome format --write",
    "lint": "biome lint",
    "upforce": "pnpm up --latest --recursive '*'",
    "up:astro": "pnpx @astrojs/upgrade",
    "up:biome": "pnpm up @biomejs/biome@latest && pnpm biome migrate --write",
    "check": "astro check",
    "cf-typegen": "wrangler types --env-interface Env",
    "clean": "rm -rf dist .astro node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@tabler/icons": "^3.36.1",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.17.1",
    "astro-cloudinary": "^1.3.5",
    "astro-font": "^1.1.0",
    "drizzle-orm": "^0.45.1",
    "nanoid": "^5.1.6",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/cloudflare-turnstile": "^0.2.2",
    "@types/node": "^25.2.0",
    "drizzle-kit": "^0.31.8",
    "typescript": "^5.9.3",
    "wrangler": "^4.62.0"
  }
}